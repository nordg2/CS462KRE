ruleset inclass {
    meta {
        name "inclass example"
        author "BJ Nordgren"
        logging off
    }
    dispatch {
         //domain "exampley.com"
    }
    rule first_rule {
        select when tweet recieved where body re/healthcare/
        // Display notification that will not fade.
        
    }
    rule second_rule {
        select when email recieved where body re/BYU/ && re/football/
    }
    rule third_rule
}
